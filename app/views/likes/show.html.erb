<body>

  <!--================Home Banner Area =================-->
 <!-- banner part start-->
 <header>
    <section class="banner_part">
        <div class="container">
            <div class="row align-items-center">
                <div class="col-lg-8">
                    <div class="banner_slider owl-carousel">
                        <div class="single_banner_slider" style="max-height: 100px">
                            <div class="row">
                                <div class="col-lg-12 col-md-12">
                                    <div class="banner_text">
                                        <div class="banner_text_iner">
                                        </div>
                                    </div>
                                </div>

                            </div>
                        </div>
                    </div>
                    <div class="slider-counter"></div>
                </div>
            </div>
        </div>
    </section>
  </header>
<!-- banner part start-->


  <!--================Home Banner Area =================-->
  <!-- breadcrumb start-->
  
  <!-- breadcrumb start-->

  <!--================Like Area =================-->
  
  <section class="cart_area padding_top">
    <div class="container">
      <div class="cart_inner">
        <div class="table-responsive">
          <table class="table">
            <div class="breadcrumb_iner_item">
              <h1>Mes projets favoris</h1>
                <body>
                  <% @like_projects.each do |like_project| %>
                  <tr>
                    <td>
                      <div class="media">
                        <div class="d-flex.text-dark">
                          <h2><%= link_to like_project.project.title , project_path(like_project.project.id) , class: "text-dark" %></h2>
                        </div>
                      </div>
                    </td>
                    <td>
                    <div class="media-body.text-dark">
                      <p><%= link_to like_project.project.description , project_path(like_project.project.id) , class: "text-dark" %></p>
                    </div>
                </td>
                <td>
                  <div class="cupon_text float-right">
                    <%= button_to "Participer au projet", attendances_path, data: { confirm: "Voulez-vous rejoindre le projet ? Un email avec les coordonnées de l'organisateur vous sera alors envoyé." , disable_with: 'Déjà rejoint' }, params: { project_id: @project }, class: "btn_1" %>
                  </div>
                </td>
              <% end %>
            </tbody>
          </table>
          </div>
        </div>
      </div>
  </section>
  <!--================End Like Area =================-->

